package coe528.lab3;

/**
 *
 * @author mbverdaw
 */
public abstract class AbstractCounter implements Counter{
    protected int value;
    
    public AbstractCounter(){
        this.value=0;
    }
    
    public abstract void increment();
    public abstract void decrement();
}
